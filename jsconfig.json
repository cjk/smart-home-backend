{
  "compilerOptions": {
    "outDir": "./app",
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "checkJs": true,
    "allowJs": true,
    "lib": ["es2017"],
    "baseUrl": "./src"
  },
  "exclude": ["node_modules", "app", "radata"],
  "include": ["src/**/*"]
}
